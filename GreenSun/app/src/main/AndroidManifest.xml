<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.wangyanci.greensun">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/q_xiong"
        android:label="@string/app_name"
        android:roundIcon="@drawable/q_xiong"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity
            android:name=".good.activity.MainActivity"
            android:label="@string/app_name">

        </activity>

        <activity
            android:name=".good.activity.LoginActivity"
            android:label="@string/app_name"

            >

        </activity>
        <activity
            android:name=".good.activity.RegisterActivity"
            android:label="注册">

        </activity>

        <activity android:name=".good.activity.SplashActivity"

            >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

        </activity>
    </application>

</manifest>